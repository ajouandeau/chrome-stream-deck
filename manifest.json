{
  "manifest_version": 2,
  "name": "Stream Deck for Chrome",
  "version": "0.0.1",
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "css": ["contentscript.css"],
      "matches": ["https://meet.google.com/*"]
    }
  ],
  "web_accessible_resources": ["assets/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
